void setup() {
  matrix.begin();
  matrix.fillRect(0, 0, 32, 32, matrix.Color333(5,5,5));
}
void loop() {
  for (uint8_t row=0; row<32; row++){
      for (uint8_t col=0; col<32; col++){
          uint8_t redindex = (int8_t)pgm_read_byte(&Red[row][col]);
          uint8_t greenindex = (int8_t)pgm_read_byte(&Green[row][col]);
          uint8_t blueindex = (int8_t)pgm_read_byte(&Blue[row][col]);
          uint16_t pixel = matrix.Color333(redindex, greenindex, blueindex);
          Serial.println(pixel);
          matrix.drawPixel(row, col, pixel);
        }
    }
}
